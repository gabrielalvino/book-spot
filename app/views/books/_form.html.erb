<%= simple_form_for @book do |f| %>
    <%= f.input :title %>
    <%= f.input :author %>
    <%= f.input :description %>
    <%= f.input :category , collection: Book::CATEGORIES%>
    <%= f.input :price %>
    <%= f.input :photo, required: true, as: :file %>
    <%= f.button :submit, class: 'btn btn-primary' %>
  <% end %>
